<h1>Virtual Hosts</h1>

<table class="list">
 <thead>
  <tr>
    <th>Name</th>
  </tr>
 </thead>
 <tbody>
  <% for (var i = 0; i < vhosts.length; i++) { %>
  <tr<%= alt_rows(i)%>>
   <td><%= link_to(vhosts[i], '#/vhosts/' + vhosts[i]) %></td>
  </tr>
  <% } %>
 </tbody>
</table>

<h2>Add a new virtual host</h2>
<%= format('vhost-form', {}) %>
