<h1>Consuming channels</h1>

<% if (stats.length > 0) { %>
<table class="list">
 <thead>
  <tr>
   <th></th>
   <th colspan="5">Message rates</th>
  </tr>
  <tr>
   <th>Channel</th>
   <th>basic.deliver</th>
   <th>basic.get</th>
   <th>basic.ack</th>
   <th>basic.deliver (noack)</th>
   <th>basic.get (noack)</th>
  </tr>
 </thead>
 <tbody>
  <% for (var i = 0; i < stats.length; i++) { %>
  <tr<%= alt_rows(i)%>>
    <td><%= stats[i].channel %></td>
    <td><%= fmt_rate(stats[i].stats, 'deliver') %></td>
    <td><%= fmt_rate(stats[i].stats, 'get') %></td>
    <td><%= fmt_rate(stats[i].stats, 'ack') %></td>
    <td><%= fmt_rate(stats[i].stats, 'deliver_noack') %></td>
    <td><%= fmt_rate(stats[i].stats, 'get_noack') %></td>
  </tr>
  <% } %>
 </tbody>
</table>
<% } else { %>
  <p>... no channels ...</p>
<% } %>
